@font-face {
	font-family: 'municipalliberia';
	src:url('./fonts/municipalliberia.eot?rczswf');
	src:url('./fonts/municipalliberia.eot?#iefixrczswf') format('embedded-opentype'),
		url('./fonts/municipalliberia.ttf?rczswf') format('truetype'),
		url('./fonts/municipalliberia.woff?rczswf') format('woff'),
		url('./fonts/municipalliberia.svg?rczswf#municipalliberia') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
	font-family: 'municipalliberia';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

.icon-twitter:before {
	content: "\f099";
}
.icon-facebook:before {
	content: "\f09a";
}
.icon-menu:before {
	content: "\f0c9";
}
.icon-google-plus:before {
	content: "\f0d5";
}
.icon-angle-left:before, .icon-arrow-left:before {
	content: "\f104";
}
.icon-angle-right:before, .icon-arrow-right:before {
	content: "\f105";
}
.icon-chevron-circle-left:before {
	content: "\f137";
}
.icon-chevron-circle-right:before {
	content: "\f138";
}
.icon-youtube:before {
	content: "\f167";
}
.icon-avotz:before {
	content: "\e600";
}
