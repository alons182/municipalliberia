		url('./fonts/municipalliberia.ttf?ekn6jx') format('truetype'),
		url('./fonts/municipalliberia.woff?ekn6jx') format('woff'),
		url('./fonts/municipalliberia.svg?ekn6jx#municipalliberia') format('svg');
